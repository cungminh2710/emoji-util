{
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "htmlparser2": "^8.0.1",
    "jest": "^29.2.2",
    "ts-jest": "^29.0.3",
    "tsup": "^6.3.0",
    "typescript": "4.8.4"
  },
  "name": "unicode-emoji-utils",
  "description": "A collection of utilities for emojis",
  "version": "1.0.1",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --treeshake --minify",
    "lint": "eslint",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest --config jestconfig.json --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cungminh2710/emoji-util.git"
  },
  "author": "Minh Cung",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cungminh2710/emoji-util/issues"
  },
  "homepage": "https://github.com/cungminh2710/emoji-util#readme",
  "dependencies": {
    "emoji-regex": "^10.2.1"
  }
}
